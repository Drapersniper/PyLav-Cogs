#: plmigrator/cog.py:33
#, docstring
msgid "Copy Red's Audio settings over to PyLav."
msgstr "Zkopírujte nastavení zvuku Redu do PyLav."

#: plmigrator/cog.py:44
#, docstring
msgid "Migrate Audio settings to PyLav\n\n"
"        Please note that this will replace any config already in PyLav.\n\n"
"        If you are sure you want to proceed please run this command again with the confirm argument set to 1\n"
"        i.e `[p]pylavmigrate 1`\n"
"        "
msgstr "Přeneste nastavení zvuku do PyLav\n\n"
"        Vezměte prosím na vědomí, že toto nahradí jakoukoli konfiguraci již v PyLav.\n\n"
"        Pokud jste si jisti, že chcete pokračovat, spusťte tento příkaz znovu s potvrzovacím argumentem nastaveným na 1\n"
"        i. `[p]pylavmigrate 1`\n"
"        "

#: plmigrator/cog.py:109
msgid "I don't have permission to access {folder}."
msgstr "Nemám oprávnění k přístupu k {folder}."

#: plmigrator/cog.py:189
msgid "Migration of Audio cog settings to PyLav complete. Restart the bot for it to take effect.\n"
"{requester}"
msgstr "Přesunutí nastavení Audio cog do PyLav dokončeno. Restartujte bot, aby se projevil účinek.\n"
"{requester}"

